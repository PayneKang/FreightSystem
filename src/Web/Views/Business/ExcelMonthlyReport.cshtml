@model FreightSystem.Models.MonthlyReportModel
@{
    ViewBag.Title = "导出月份报表";
}
<script language="javascript" type="text/javascript" src="/Scripts/My97DatePicker/WdatePicker.js"></script>
<br />
@using (Html.BeginForm("ExcelMonthlyReport", "Business", FormMethod.Post))
{
    <fieldset class="large-5">
        <legend>@ViewBag.Title</legend>
        <div class="row">
            <br />
            <div class="large-6 columns large-offset-1">
                <label>
                    日期</label>
                @Html.TextBoxFor(x => x.YearMonth, new { @onClick = "WdatePicker({dateFmt:'yyyy-MM'})" })
                @Html.ValidationMessageFor(x => x.YearMonth)
            </div>
            <div class="large-5 columns">
                <label style="visibility: hidden">
                    样式调整</label>
                <button type="submit" class="button tiny" id="btnExport">
                    导出月报表</button>
            </div>
        </div>
    </fieldset>
}